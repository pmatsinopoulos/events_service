<tr>
  <td>
    <%= link_to event.name, event_path(event) %>
  </td>
  <td>
    <%= event.event_type.humanize %>
  </td>
  <td>
    <%= event.starts_at %>
  </td>
  <td>
    <%= event.ends_at %>
  </td>
  <td>
    <%= event.address %>
  </td>
  <td>
    <%= link_to "Show", event %>
  </td>
  <td>
    <%= link_to "Edit", edit_event_path(event) %>
  </td>
  <td>
    <%= link_to "Delete", event, method: :delete, data: { confirm: "Are you sure?" } %>
  </td>
</tr>
